import java.util.*;

class Solution {
    public int solution(int[] A) {
        int[] B = new int[A.length];

        for (int i = 0; i < B.length; i++){
            B[i] = i + 1;
        }

        Arrays.sort(A);

        if (Arrays.equals(A,B)){
            return 1;
        } 
        return 0;
    }
}
